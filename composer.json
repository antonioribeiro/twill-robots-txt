{
  "name": "area17/twill-robots-txt",
  "description": "A Twill Capsule to add and handle Robots TXT",
  "homepage": "https://github.com/area17/twill-robots-txts",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "area17/twill": "^2.7|^3.0"
  },
  "require-dev": {
      "nunomaduro/larastan": "^2.0",
      "phpstan/phpstan": "^1.8",
      "orchestra/testbench": "^7.11",
      "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "A17\\TwillRobotsTxt\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "A17\\TwillRobotsTxt\\ServiceProvider"
      ]
    }
  },
  "scripts": {
      "post-autoload-dump": [
          "npx husky install"
      ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
